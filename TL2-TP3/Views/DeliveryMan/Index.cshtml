@{
    ViewData["Title"] = "Our Delivery Mans";
}
@model List<TL2_TP3.Models.DeliveryMan>

<table class="table table-bordered">
    <thead class="thead-dark">
        <tr>
            <td>LastName</td>
            <td>Name</td>
            <td>Address</td>
            <td>Phone</td>
            <td>Ordeers</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><b>@item.Id</b></td>
                <td>@item.Name</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>
                <td>@item.Orders.Count</td>
            </tr>
        }
    </tbody>
</table>